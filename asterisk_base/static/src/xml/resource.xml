<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

  <t t-name="CodeMirror">
    <t t-if="widget.get('effective_readonly')">
        <i><span class="oe_form_char_content"></span></i>
    </t>
  </t>

  <t t-name="AsteriskConf">
    <textarea id="asterisk_conf" class="asterisk_conf"></textarea>
  </t>

  <t t-name="ServerCli">
    <div class="container">
        <pre>Asterisk Console</pre>
        <div class="terminal-container" id="terminal-container"></div>
    </div>
  </t>

</templates>
